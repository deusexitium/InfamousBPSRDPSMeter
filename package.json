{
  "name": "infamous-bpsr-dps-meter",
  "version": "3.1.83",
  "description": "Infamous BPSR DPS Meter - The Ultimate Blue Protocol DPS Tracker",
  "main": "electron-main.js",
  "bin": {
    "bpsr-meter": "server.js"
  },
  "engines": {
    "node": "^22.15.0"
  },
  "scripts": {
    "build": "pkg .",
    "start": "electron .",
    "lint:pritter": "prettier --write .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dist": "electron-builder --win --x64",
    "build-msi": "bash build-msi-from-wsl.sh"
  },
  "keywords": [],
  "author": "Infamous <https://github.com/ssalihsrz/InfamousBPSRDPSMeter>",
  "contributors": [
    "MrSnakeVT",
    "NeRooNx <https://github.com/NeRooNx>",
    "winjwinj <https://github.com/winjwinj/bpsr-logs>"
  ],
  "license": "AGPL-3.0",
  "packageManager": "pnpm@10.13.1",
  "dependencies": {
    "cap": "^0.2.1",
    "cors": "^2.8.5",
    "electron-updater": "^6.6.2",
    "express": "^5.1.0",
    "google-protobuf": "^4.0.0",
    "long": "^5.3.2",
    "protobufjs": "^7.5.4",
    "socket.io": "^4.8.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@yao-pkg/pkg": "^6.6.0",
    "electron": "^38.0.0",
    "electron-builder": "^26.0.12",
    "prettier": "^3.6.2",
    "protobufjs-cli": "^1.1.3"
  },
  "pkg": {
    "scripts": "server.js",
    "assets": [
      "public/**/*",
      "tables/**/*",
      "diccionario.json",
      "ip.txt",
      "player_map.json",
      "settings.json",
      "users.json",
      "node_modules/cap/**/*"
    ],
    "targets": [
      "node22-win-x64"
    ],
    "outputPath": "dist"
  },
  "build": {
    "appId": "com.infamous.bpsr.dps.meter",
    "productName": "Infamous BPSR DPS Meter",
    "artifactName": "InfamousBPSRDPSMeter-Setup-${version}.${ext}",
    "files": [
      "electron-main.js",
      "preload.js",
      "server.js",
      "src/**/*",
      "algo/**/*",
      "tables/**/*",
      "public/**/*",
      "diccionario.json",
      "player_map.json",
      "settings.json",
      "users.json",
      "icon.ico",
      "icon.png",
      "node_modules/**/*"
    ],
    "asarUnpack": [
      "node_modules/cap/**/*"
    ],
    "extraResources": [
      {
        "from": "server.js",
        "to": "app/server.js"
      },
      {
        "from": "src",
        "to": "app/src"
      },
      {
        "from": "algo",
        "to": "app/algo"
      },
      {
        "from": "tables",
        "to": "app/tables"
      },
      {
        "from": "public",
        "to": "app/public"
      },
      {
        "from": "diccionario.json",
        "to": "app/diccionario.json"
      },
      {
        "from": "medidor.png",
        "to": "app/medidor.png"
      },
      {
        "from": "portada.png",
        "to": "app/portada.png"
      },
      {
        "from": "restart-npcap.bat",
        "to": "restart-npcap.bat"
      },
      {
        "from": "check-dependencies.ps1",
        "to": "check-dependencies.ps1"
      },
      {
        "from": "pre-launch.bat",
        "to": "pre-launch.bat"
      }
    ],
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "icon.ico",
      "requestedExecutionLevel": "requireAdministrator",
      "signAndEditExecutable": false,
      "verifyUpdateCodeSignature": false
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "Infamous BPSR DPS Meter",
      "perMachine": true,
      "runAfterFinish": true,
      "deleteAppDataOnUninstall": false,
      "allowElevation": true,
      "installerIcon": "icon.ico",
      "uninstallerIcon": "icon.ico",
      "installerHeaderIcon": "icon.ico",
      "packElevateHelper": true,
      "displayLanguageSelector": false,
      "menuCategory": true,
      "installerHeader": "icon.ico",
      "installerSidebar": "icon.ico",
      "warningsAsErrors": false
    },
    "publish": {
      "provider": "github",
      "owner": "ssalihsrz",
      "repo": "InfamousBPSRDPSMeter",
      "releaseType": "release"
    },
    "directories": {
      "output": "dist_electron"
    }
  }
}
